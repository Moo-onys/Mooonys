                        <div class="position-relative">
                            <input
                                class="reset expandable-search__input js-expandable-search__input js-autocomplete__input"
                                type="text" name="autocomplete-input" id="autocomplete-input" placeholder="Search"
                                autocomplete="off">

                            <div class="autocomplete__loader sr-only position-absolute top-0 right-0 padding-right-sm height-100% flex items-center"
                                aria-hidden="true">
                                <div class="circle-loader circle-loader--v1">
                                    <div class="circle-loader__circle"></div>
                                </div>
                            </div>
                        </div>

                        <button class="reset expandable-search__btn">
