                                                    <circle cx="15" cy="15" r="15" fill="var(--color-contrast-lower)" />
                                                    <path
                                                        d="M 25.607 4.393 L 4.393 25.607 A 15 15 0 0 0 25.607 4.393 Z M 19 11 A 1 1 0 0 0 12 18" />
